{
  "name": "@luolapeikko/core-env-vite",
  "version": "0.0.1",
  "description": "Vite loader for EnvKit",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --sourcemap --format esm --dts --clean -c tsconfig.build.json",
    "validate": "tsc --noEmit"
  },
  "keywords": [
    "env",
    "environment",
    "variables",
    "config",
    "configuration",
    "loader",
    "parser",
    "typescript",
    "schema",
    "validation",
    "envkit"
  ],
  "author": "mharj",
  "license": "LGPL-2.1-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luolapeikko/core-ts.git"
  },
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@luolapeikko/core-env": "workspace:*"
  },
  "peerDependencies": {
    "@luolapeikko/core-env": ">= 0.0.1",
    "@luolapeikko/result-option": ">= 2.1.1",
    "vite": ">= 6.0.0"
  }
}

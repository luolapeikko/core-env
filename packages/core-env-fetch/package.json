{
  "name": "@luolapeikko/core-env-fetch",
  "version": "0.0.2",
  "description": "Fetch loader for EnvKit",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --sourcemap --format cjs,esm --dts --clean -c tsconfig.build.json",
    "validate": "tsc --noEmit"
  },
  "keywords": [
    "env",
    "environment",
    "variables",
    "config",
    "configuration",
    "loader",
    "parser",
    "typescript",
    "schema",
    "validation",
    "envkit"
  ],
  "author": "mharj",
  "license": "LGPL-2.1-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luolapeikko/core-ts.git"
  },
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@luolapeikko/core-env": "workspace:*",
    "etag": "^1.8.1"
  },
  "peerDependencies": {
    "@avanio/logger-like": ">= 0.1.0",
    "@luolapeikko/core-env": ">=0.0.2",
    "@luolapeikko/core-ts-error": ">= 0.0.4",
    "@luolapeikko/core-ts-loadable": ">= 0.0.4",
    "@luolapeikko/core-ts-record": ">= 0.0.4",
    "@luolapeikko/core-ts-type": ">= 0.0.4",
    "@luolapeikko/result-option": ">= 2.1.1",
    "@standard-schema/spec": ">= 1.0.0"
  }
}
